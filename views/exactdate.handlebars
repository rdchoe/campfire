<body>
    <h2 id = "browse">Search by date: </h2>
    <div id = "browse-divider"></div>
    <br>

<!--    <p>Date: <input type="text" id="datepicker"></p>-->
    <p>Enter Date: <input id="search-date" type="date"></p>
    <hr width="100%">

    <br>

    <div class="wrap-table100">
    <div class="table100">
        <table id = "channel_table">
        <thead>
        <tr class="table100-head">
            <th class="column1">Channel</th>
            <th class="column2">Date Created</th>
            <th class="column3">Genre</th>
            <th class = "column4">Active Listeners</th>
        </tr>
        </thead>
        <tbody>
            {{#each channels}}
            <tr onclick="location.href='/channel/{{this.name}}';">
                <td class = "column1">{{this.name}} </td>
                <td class = "column2">{{dateFormat this.date_created "dddd, MMMM Do YYYY"}}</td>
                <td class = "column3">{{this.genre}}</td>
                <td class ="column4">{{this.active}}</td>
                </tr>
            {{/each}}                 
        </tbody>
        </table>
    </div>
    </div>

